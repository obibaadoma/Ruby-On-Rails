<h1>Listing Students</h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th colspan='3'> Actions/<th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
    <tr>
      <td>
        <%= student.first_name %>
        <%= student.last_name %>
        <%= student.email %>
        <td>
          <%= link_to 'view', student %>
        </td>
        <td>
      </tr>
    <% end %>
  </tbody>
</table>